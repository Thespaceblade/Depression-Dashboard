{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": null,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11",
      "includeFiles": "teams_config.json,depression_calculator.py,sports_api.py"
    }
  },
  "crons": [
    {
      "path": "/api/cron/fetch-data",
      "schedule": "0 */6 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
